{
  "name": "breweries",
  "description": "Search breweries in the USA by name, state, city, and type. Find breweries near you using geolocation. Save favorites to local storage.",
  "keywords": ["api", "angular", "breweries", "geolocation", "localStorage", "typescript", "rxjs"],
  "author": {
    "name": "NightWolf Software Development",
    "url": "https://nightwolf.dev"
  },
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --base-href /breweries/",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@cds/core": "^6.14.0",
    "@clr/angular": "^17.4.0",
    "@clr/ui": "^17.4.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.2",
    "@angular/cli": "^19.0.2",
    "@angular/compiler-cli": "^19.0.0",
    "typescript": "~5.6.2"
